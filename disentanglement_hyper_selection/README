1.  module load miniconda
2.  conda activate as3465_1
3.  module load dSQ
4.  export LD_LIBRARY_PATH=/gpfs/loomis/project/karunakaran/as3465/conda_envs/as3465_1/lib:$LD_LIBRARY_PATH
5.  cp ../dataset_creation/edited_files/*npz data/watches/
6.  vim hyperparam.ini [ put: sup_signal = "brand" ]
6.  dsq --job-file <file_name> --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00 ## Run the models
    Example: dsq --job-file for_python_script_files/classification_models_c1.txt --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00
7.  cd results; mkdir brand; mv s* brand; cp mv_script.sh brand; ./mv_script.sh; cd .. 
8.  vim hyperparam.ini [ put: sup_signal = "circa" ]
9.  dsq --job-file <file_name> --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00 ## Run the models
    Example: dsq --job-file for_python_script_files/classification_models_c1.txt --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00
10. cd results; mkdir circa; mv s* circa; cp mv_script.sh circa; ./mv_script.sh; cd ..
11. vim hyperparam.ini [ put: sup_signal = "movement" ]
12. dsq --job-file <file_name> --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00 ## Run the models
    Example: dsq --job-file for_python_script_files/classification_models_c1.txt --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00
13. cd results; mkdir movement; mv s* movement; cp mv_script.sh movement; ./mv_script.sh; cd ..
14. vim hyperparam.ini [ put: sup_signal = "material" ]
15. dsq --job-file <file_name> --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00 ## Run the models
    Example: dsq --job-file for_python_script_files/classification_models_c1.txt --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00
16. cd results; mkdir material: mv s* material; cp mv_script.sh material; ./mv_script.sh, cd ..
17. vim hyperparam.ini [ put: sup_signal = "price" ]
18. dsq --job-file <file_name> --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00 ## Run the models
    Example: dsq --job-file for_python_script_files/classification_models_c1.txt --mem=64G --gres=gpu:1 --partition=gpu -t 15:00:00
19. cd results; mkdir price: mv s* price; cp mv_script.sh price; ./mv_script.sh; cd ..
